                                    </thead>
                                    <tfoot>
                                        <tr>
                                             <th>product_id</th>
                                            <th>product_name</th>
                                            <th>product_image</th>
                                            <th>supplier_id</th>
                                            <th>cartons</th>
                                            <th>Unit_per_carton</th>
                                            <th>unit_cost</th>
                                            <th>Quantity</th>
                                            <th>total_cost</th>
                                            <th>Action</th>
                                        </tr>
                                    </tfoot>
                                    <tbody>
                                        <% if(purchases && purchases.length > 0){%>
                                            <% purchases.forEach((product)=>{
                                                const image = product.product_image ? product.product_image : 'undraw_profile.png'; // Fallback image if none exists
